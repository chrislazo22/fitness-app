<h1>Here is your Current Workout Plan </h1>

<%=  @current_workout_plan.each do |key, value| %>
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">
      	<%= key %>
      </h3>
    </div>
    <div class="panel-body">
      <% value.each do |i| %>
        <br><%= i.name if key == key %></br>
      <% end %>
    </div>
  </div>
<% end %>
